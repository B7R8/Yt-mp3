{
  "name": "youtube-mp3-converter-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:optimized": "node scripts/build-optimized.js",
    "preview": "vite preview",
    "analyze": "npm run build && npx vite-bundle-analyzer dist/assets/*.js",
    "tailwind:build": "tailwindcss -i ./input.css -o ./dist/output.css --minify",
    "tailwind:watch": "tailwindcss -i ./input.css -o ./dist/output.css --watch"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "swr": "^2.2.4"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.14",
    "@tanstack/react-query-devtools": "^5.0.0",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "cssnano": "^7.1.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.14",
    "terser": "^5.44.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}